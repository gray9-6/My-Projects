{
    "name": "being qurious",
    "description": "A chrome extension for efficient browsing",
    "version": "1.0",
    "manifest_version": 3,
    "icons": {
        "16": "images/icon-16.png",
        "32": "images/icon-32.png",
        "64": "images/icon-64.png",
        "128": "images/icon-128.png"
    },
    "content_scripts": [
        {
            "js": ["scripts/content.js"],
            "css": ["scripts/content.css"],
            "matches": ["*://*/*"]
        }
    ]
}
